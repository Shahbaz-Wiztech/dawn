                            .swiper-wrapper {
                                .swiper-slide {

                                    .post-card-holder {
                                        .post-card-img {
                                            height: 180px;
                                            width: 100%;
                                            overflow: hidden;

                                            &:hover {
                                                img {

                                                    scale: (1.1);
                                                    filter: contrast(0.7);
