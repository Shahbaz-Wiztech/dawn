<section class="brandsHolder">
  <div class="container">
    <div class="brandsSec">
      <div class="brandImg">
        {% if section.settings.image_1 %}
          <img
            src="{{ section.settings.image_1 |  img_url:'master' }}"
            height="auto"
            width="auto"
            alt="{{ section.settings.image_1.alt }}"
          >
        {% else %}
          <img src="{{ 'bloom.png' |  asset_url }}" height="auto" width="auto" alt="bloom image">
        {% endif %}
      </div>
      <div class="brandImg">
        {% if section.settings.image_2 %}
          <img
            src="{{ section.settings.image_2 |  img_url:'master' }}"
            height="auto"
            width="auto"
            alt="{{ section.settings.image_2.alt }}"
          >
        {% else %}
          <img src="{{ 'monster.png' |  asset_url }}" height="auto" width="auto" alt="monster image">
        {% endif %}
      </div>
      <div class="brandImg">
        {% if section.settings.image_3 %}
          <img
            src="{{ section.settings.image_3 |  img_url:'master' }}"
            height="auto"
            width="auto"
            alt="{{ section.settings.image_3.alt }}"
          >
        {% else %}
          <img src="{{ 'silverstone.png' |  asset_url }}" height="auto" width="auto" alt="silver stone image">
        {% endif %}
      </div>
      <div class="brandImg">
        {% if section.settings.image_4 %}
          <img
            src="{{ section.settings.image_4 |  img_url:'master' }}"
            height="auto"
            width="auto"
            alt="{{ section.settings.image_4.alt }}"
          >
        {% else %}
          <img src="{{ 'modes.png' |  asset_url }}" height="auto" width="auto" alt="mades image">
        {% endif %}
      </div>
      <div class="brandImg">
        {% if section.settings.image_5 %}
          <img
            src="{{ section.settings.image_5 |  img_url:'master' }}"
            height="auto"
            width="auto"
            alt="{{ section.settings.image_5.alt }}"
          >
        {% else %}
          <img src="{{ 'mangler.png' |  asset_url }}" height="auto" width="auto" alt="mangler image">
        {% endif %}
      </div>
      <div class="brandImg">
        {% if section.settings.image_6 %}
          <img
            src="{{ section.settings.image_6 |   img_url: 'master' }}"
            height="auto"
            width="auto"
            alt="{{ section.settings.image_6.alt }}"
          >
        {% else %}
          <img src="{{ 'winston.png' |  asset_url }}" height="auto" width="auto" alt="winston image">
        {% endif %}
      </div>
    </div>
  </div>
</section>

{% schema %}
{
  "name": "Brands Logo Section",
  "settings": [
    {
      "type": "image_picker",
      "id": "image_1",
      "label": "Brand Logo 1"
    },
    {
      "type": "image_picker",
      "id": "image_2",
      "label": "Brand Logo 2"
    },
    {
      "type": "image_picker",
      "id": "image_3",
      "label": "Brand Logo 3"
    },
    {
      "type": "image_picker",
      "id": "image_4",
      "label": "Brand Logo 4"
    },
    {
      "type": "image_picker",
      "id": "image_5",
      "label": "Brand Logo 5"
    },
    {
      "type": "image_picker",
      "id": "image_6",
      "label": "Brand Logo 6"
    }
  ],
  "presets": [{ "name": "Brands Logo Section" }]
}
{% endschema %}
