                            <a href="#" class="menu-title">
                              <h6>Category</h6>
                            </a>
                          </li>
                          {% for collection in collections %}
                            <li>
                              <a href="{{ collection.url }}">{{ collection.title }}</a>
                            </li>
                          {% endfor %}
                        </ul>
                      </div>
                      <div class="col-md-4 dropdown-menu-brands-lens p-1">
                        <ul class="d-flex flex-column align-items-start">
                          <li>
                            <a href="#" class="menu-title">
