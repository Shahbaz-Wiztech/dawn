              {%- render 'icon-search' -%}
            </a>
          </div>
        </form>
      </div>
      <div class="modal-footer border-top-0">
        <a href="{{ routes.collections_url }}" type="button" class="btn black m-auto">View all categories</a>
      </div>
    </div>
  </div>
</div>
{% comment %} Responsive {% endcomment %}
<div class="modal w-screen-modal fade" id="responsiveModal" tabindex="-1" aria-labelledby="responsiveModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header border-bottom-0">
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body d-flex flex-column align-items-center justify-content-center">
        <form class="w-100">
          {% comment %} <div class="w-100 d-flex align-items-center justify-content-center position-relative"> {% endcomment %}
            <input type="text" class="form-control pb-2" placeholder="Type here...">
            {% comment %} <a href="javascript:void(0);" class="position-absolute end-0">
              {%- render 'icon-search' -%}
            </a>
          </div> {% endcomment %}
        </form>
        <div class="custom-tabs">
          <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item" role="presentation">
              <button class="nav-link active" id="menu-tab" data-bs-toggle="tab" data-bs-target="#menu" type="button" role="tab" aria-controls="menu" aria-selected="true">menu</button>
            </li>
            <li class="nav-item" role="presentation">
              <button class="nav-link" id="categories-tab" data-bs-toggle="tab" data-bs-target="#categories" type="button" role="tab" aria-controls="categories" aria-selected="false">categories</button>
            </li>
          </ul>
          <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="menu" role="tabpanel" aria-labelledby="menu-tab">
              <div class="d-flex flex-column align-items-center justify-content-center gap-2 mt-2">
                {% for link in section.settings.menu.links %}
                  <a href="{{ link.url }}" class="text-start w-100 text-decoration-none text-uppercase">{{ link.title }}</a>  
                {% endfor %}
                </div>
            </div>
            <div class="tab-pane fade" id="categories" role="tabpanel" aria-labelledby="categories-tab">...</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

{% comment %} <div class="relative bg-white">
  <div class="mx-auto px-4 sm:px-6">
    <div class="flex justify-center items-center py-6 md:justify-start md:space-x-10"> {% endcomment %}
      {% comment %} Store Name {% endcomment %}
      {% comment %} <div class="flex justify-start lg:flex-1 truncate">
        <h1 class="text-xl font-bold truncate">{{ shop.name }}</h1>
      </div> {% endcomment %}
      {% comment %} Navigation Menu {% endcomment %}
      {% comment %} <div class="hidden md:flex space-x-10">
        {% for link in section.settings.menu.links %}
          {% if link.links != blank %}
            <div class="relative" x-data="{open:false}">
              <button x-on:click="open = !open" class="inline-flex items-center">
                {{ link.title }}
                {% render 'icon-dropdown' %}
              </button>
              <div
                x-transition:enter="transition ease-out duration-300"
                x-transition:enter-start="opacity-0 translate-y-3"
                x-transition:enter-end="opacity-100 translate-y-0"
                x-transition:leave="transition ease-out duration-300"
                x-transition:leave-start="opacity-100 translate-y-0"
                x-transition:leave-end="opacity-0 translate-y-3"
                x-show="open"
                @click.away="open=false"
                class="absolute z-10 -ml-5 transform max-w-md sm:px-0 lg:left-1/2"
              >
                <div class="shadow-lg border ring-black ring-opacity-5 overflow-hidden rounded-lg">
                  <div class="relative grid gap-6 bg-white px-5 sm:p-8">
                    {% for childlink in link.links %}
                      <a href="{{ childlink.url }}" class="-m-3 p-3 flex items-start rounded-lg hover:bg-gray-100">
                        <div class="ml-4">
                          <p class="text-gray-900 text-base font-medium">
                            {{ childlink.title }}
                          </p>
                        </div>
                      </a>
                    {% endfor %}
                  </div>
                </div>
              </div>
            </div>
          {% else %}
            <a href="{{ link.url }}" class="text-base text-gray-500 hover:text-gray-700">{{ link.title }}</a>
          {% endif %}
        {% endfor %}
      </div> {% endcomment %}
      {% comment %} cart button / account links if enabled {% endcomment %}
      {% comment %} <div class="hidden md:flex items-center justify-end md:flex-1 space-x-1">
        <a href="{{ routes.cart_url }}" class="whitespace-nowrap text-base text-gray-500 hover:text-gray-500">
          {%- render 'icon-shopping-bag' -%}
        </a>
      </div> {% endcomment %}
      {% comment %} {% if shop.customer_accounts_enabled %}
        {% if customer %}
          <a href="{{ routes.account_url }}" class="text-base text-gray-500 hover:text-gray-700">accounts</a>
          {{
            'logout'
            | customer_logout_link
            | replace: '<a', '<a class="text-base text-gray-500 hover:text-gray-700"'
          }}
        {% else %}
          {{ 'login' | customer_login_link | replace: '<a', '<a class="text-base text-gray-500 hover:text-gray-700"' }}
          {{
            'Register'
            | customer_register_link
            | replace: '<a', '<a class="text-base text-gray-500 hover:text-gray-700"'
          }}
        {% endif %}
      {% endif %} {% endcomment %}
    {% comment %} </div>
  </div>
</div> {% endcomment %}

{% schema %}
