    {
      "type": "image_picker",
      "id": "logo",
      "label": "Upload Logo"
    },
    {
      "type": "link_list",
      "id": "menu",
      "default": "main-menu",
      "label": "Heading Navigation Menu"
    }
  ],
  "blocks":[
    {
      "type": "announcementbar",
      "name": "Announcement block",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
        {
          "type": "text",
          "id": "image_alt",
          "label": "Image Alternative Text"
        },
        {
          "type": "text",
          "id": "title",
          "label": "Title"
        },
        {
          "type": "text",
          "id": "button",
          "label": "Button Text"
        },
        {
          "type": "url",
          "id": "url",
          "label": "Button URL"
        }
      ]
    },
    {
      "type": "social_icons",
      "name": "Social Icons",
      "settings": [
        {
          "type": "image_picker",
          "id": "icon_image",
          "label": "Icon"
        },
        {
          "type": "text",
          "id": "icon_image_alt",
          "label": "Icon Alternative Text"
        },
        {
          "type": "url",
          "id": "icon_url",
          "label": "Icon Url"
        }
      ]
    },
  ],
  "presets": [
        {
          "name": "Social Icons"
        }
      ]
}
{% endschema %}
