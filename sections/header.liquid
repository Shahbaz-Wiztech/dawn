<div class="main-header bg-white py-3 py-md-4 border-bottom">
  <div class="container">
    <div class="row">
      <div class="d-block d-md-none col-2">
        <a href="javascript:void(0);" data-bs-toggle="modal" data-bs-target="#responsiveModal">
          {%- render 'icon-hamburger' -%}
        </a>
        {% comment %}
          <div class="hamburger-menu" x-data="{open:false}">
              <span x-on:click="open = !open">
                {%- render 'icon-hamburger' -%}
              </span>
              <div
                x-show="open"
                @click.away="open=false"
                class="custom-menu p-2"
              >
                <div class="w-100 d-flex flex-column align-items-center justify-content-start">
                  {% for link in section.settings.menu.links %}
                    <a href="{{ link.url }}" class="text-start w-100 text-decoration-none text-uppercase">{{ link.title }}</a>
                  {% endfor %}
                </div>
              </div>
          </div>
        {% endcomment %}
      </div>
      <div class="col-8 col-md-3">
        <a href="/" class="flex justify-start lg:flex-1 truncate">
          {% if section.settings.logo %}
            <img
              src="{{ section.settings.logo |  img_url: 'master' }}"
              alt="{{ shop.name }}"
              width="auto"
              height="auto"
            >
          {% else %}
            <h1 class="">{{ shop.name }}</h1>
          {% endif %}
        </a>
      </div>
      <div class="d-none d-md-flex col-md-6">
        <div class="d-flex gap-4 align-items-center justify-content-center h-100 menu-list w-100">
          {% for link in section.settings.menu.links %}
            {% assign mega_menu = false %}
            {% for block in section.blocks %}
              {% case block.type %}
                {% when 'mega_menu' %}
                  {% if block.settings.title == link.title %}
                    {% assign mega_menu = true %}
                    <div class="relative mega-dropdown">
                      <a href="{{ link.url }}" class="menu-list-item">
                        {{ link.title }}
                        {% render 'icon-dropdown' %}
                      </a>
                      <div class="dropdownMenuContainer">
                        <div class="dropdownMenu">
                          <div class="row px-2">
                            <div class="col-md-4 dropdown-menu-category p-1">
                              <ul class="d-flex flex-column align-items-start">
                                <li>
                                  <a href="#" class="menu-title">
                                    <h6>Category</h6>
                                  </a>
                                </li>
                                {% for category in block.settings.category_list %}
                                  <li>
                                    <a href="{{ category.url }}">
                                      {{- category.title -}}
                                    </a>
                                  </li>
                                {% endfor %}
                              </ul>
                            </div>
                            <div class="col-md-4 dropdown-menu-brands-lens p-1">
                              <ul class="d-flex flex-column align-items-start">
                                <li>
                                  <a href="#" class="menu-title">
                                    <h6>Brand</h6>
                                  </a>
                                </li>
                                {% for brand in block.settings.brands_list.links %}
                                  <li>
                                    <a href="{{ brand.url }}">
                                      {{- brand.title -}}
                                    </a>
                                  </li>
                                {% endfor %}
                              </ul>
                              <ul class="d-flex flex-column align-items-start pt-4">
                                <li>
                                  <a href="#" class="menu-title">
                                    <h6>Contact Lenses</h6>
                                  </a>
                                </li>
                                {% for lenses in block.settings.contact_lenses.links %}
                                  <li>
                                    <a href="{{ lenses.url }}">
                                      {{- lenses.title -}}
                                    </a>
                                  </li>
                                {% endfor %}
                              </ul>
                            </div>
                            <div class="col-md-4 dropdown-menu-link-img p-1">
                              {% if block.settings.mega_menu_image %}
                                <img
                                  src="{{ block.settings.mega_menu_image |  img_url:'master' }}"
                                  height="auto"
                                  width="auto"
                                  alt="{{ block.settings.mega_menu_image.alt }}"
                                >
                              {% else %}
                                <img
                                  src="{{ 'john-jacobs.jpg' |  asset_url }}"
                                  height="auto"
                                  width="auto"
                                  alt="Modal Image"
                                >
                              {% endif %}
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  {% endif %}
              {% endcase %}
            {% endfor %}
            {% if mega_menu == false %}
              {% if link.links != blank %}
                <div class="dropdown">
                  <a
                    class="dropdown-toggle"
                    href="#"
                    id="navbarDropdown"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-expanded="false"
                  >
                    {{ link.title }}
                  </a>
                  <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                    {% for childlink in link.links %}
                      <li>
                        <a class="dropdown-item" href="{{ childlink.url }}">{{ childlink.title }}</a>
                      </li>
                    {% endfor %}
                  </ul>
                </div>
              {% else %}
                <a href="{{ link.url }}" class="menu-list-item">{{ link.title }}</a>
              {% endif %}
            {% endif %}
          {% endfor %}
        </div>
      </div>
      <div class="col-2 col-md-3">
        <div class="d-flex align-items-center justify-content-center gap-0 gap-md-4 h-100">
          <a href="javascript:void(0);" class="d-none d-md-block" data-bs-toggle="modal" data-bs-target="#searchModal">
            {%- render 'icon-search' -%}
          </a>
          <a href="javascript:void(0);" class="position-relative d-none d-md-block">
            <span class="position-absolute top-0 start-100 translate-middle badge rounded-pill bg-primary"> 77 </span>
            {%- render 'icon-favorite' -%}
          </a>
          <a
            class="position-relative"
            href="javascript:void(0);"
            type="button"
            data-bs-toggle="offcanvas"
            data-bs-target="#offcanvasScrolling"
            aria-controls="offcanvasScrolling"
          >
            <span class="position-absolute top-0 start-100 translate-middle badge rounded-pill bg-primary">
              {{ cart.item_count }}
            </span>
            {%- render 'icon-shopping-bag' -%}
          </a>
        </div>
      </div>
    </div>
  </div>
</div>
